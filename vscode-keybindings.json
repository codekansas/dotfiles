[
  // Cursor move up.
  {
    "key": "alt+up",
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "line",
      "value": 5
    },
    "when": "editorTextFocus"
  },
  // Cursor move down.
  {
    "key": "alt+down",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 5
    },
    "when": "editorTextFocus"
  },
  // Copy relative file path.
  {
    "key": "cmd+shift+c",
    "command": "copyRelativeFilePath",
    "when": "editorTextFocus"
  },
  // Save without formatting.
  {
    "key": "cmd+e",
    "command": "workbench.action.files.saveWithoutFormatting",
    "when": "editorTextFocus"
  },
  // Cursor composer modes
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  },
  {
    "key": "cmd+e",
    "command": "composerMode.background"
  }
]
